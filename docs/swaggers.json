{
    "openapi": "3.0.2",
    "info": {
        "title": "CarAds-API",
        "version": "1.0.0",
        "description": "This is a Swagger UI for our Cars ads API"
    },
    "paths": {
        "/cars": {
            "get": {
                "tags": [
                    "cars"
                ],
                "responses": {
                    "200": {
                        "content": {
                            "application/json": {
                                "schema": {
                                    "type": "array",
                                    "items": {
                                        "type": "string"
                                    }
                                },
                                "examples": {
                                    "List of car ads": {
                                        "value": [
                                            "Audi a5",
                                            "BMW 3",
                                            "Mercedes c"
                                        ]
                                    }
                                }
                            }
                        }
                    }
                },
                "description": "Returns a list of car ads\n"
            }
        }
    }
}
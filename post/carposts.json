{
    "openapi": "3.0.2",
    "info": {
        "title": "CarPost-API",
        "version": "1.0.0",
        "description": "Post a car ad"
    },
    "paths": {
        "/post": {
            "post": {
                "tags": [
                    "post"
                ],
                "responses": {
                    "201": {
                        "content": {
                            "application/json": {
                                "schema": {
                                    "type": "array",
                                    "items": {
                                        "type": "string"
                                    }
                                },
                                "examples": {
                                    "Post ad": {
                                        "value": [
                                            "Car",
                                            "Price",
                                            "Seller",
                                            "Owner",
                                            "Location"
                                        ]
                                    }
                                }
                            }
                        },
                        "description": "Post Created Successfully"
                    }
                },
                "summary": "Post a car ad",
                "description": "Post ads\n"
            }
        }
    }
}